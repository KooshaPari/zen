# Minimal .env for running without OAuth (development only)
# Source from parent .env and override auth settings

# Load all settings from main .env
$(cat .env | grep -v "^ENABLE_OAUTH2_AUTH" | grep -v "^ENABLE_HTTP_OAUTH" | grep -v "^REQUIRE_DEVICE_AUTH" | grep -v "^ENABLE_DEVICE_AUTH")

# Disable OAuth for local development
ENABLE_OAUTH2_AUTH=false
ENABLE_HTTP_OAUTH=0
REQUIRE_DEVICE_AUTH=false
ENABLE_DEVICE_AUTH=false
ENFORCE_ACL=0