# Test-specific environment overrides to ensure deterministic test behavior.
# These values are sourced by test scripts and take precedence over .env.

# Providers disabled by default
OPENAI_API_KEY=
GEMINI_API_KEY=
XAI_API_KEY=
OPENROUTER_API_KEY=
DIAL_API_KEY=
CUSTOM_API_URL=
CUSTOM_API_KEY=
CUSTOM_MODEL_NAME=
DEFAULT_MODEL=auto

# Core infrastructure
# Ports mirror current values from .env
NATS_PORT=4222
NATS_MONITOR_PORT=8222
REDIS_PORT=6379
NATS_SERVERS=nats://localhost:4222
REDIS_URL=redis://localhost:6379/0

# HTTP auth & ACLs disabled for tests
ENABLE_HTTP_OAUTH=0
ENFORCE_ACL=0
HTTP_HMAC_SECRET=

# Misc toggles
A2A_ENABLED=0
LOG_LEVEL=WARNING
